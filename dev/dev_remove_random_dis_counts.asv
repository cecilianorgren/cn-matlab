av_counts_lowE = mean(sum(iPDist3_counts.elim(elim_lowE).nan2zero.data,[3 4]),1);
av_counts_lowE_1 = mean(av_counts_lowE);

f_per_count = iPDist3./iPDist3_cou



fontsize = 14;

hca = subplot(1,1,1);
plot(hca,iPDist3_counts.depend{1}(1,:),av_counts)
hold(hca,'on')
hl = plot(hca,iPDist3_counts.elim(elim_lowE).depend{1}(1,:),av_counts_lowE,'*');
hold(hca,'off')
hca.XLabel.String = 'Energy (eV)';
