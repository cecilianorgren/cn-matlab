function varargout = f_inp(input)

switch 
  case 'slow_e'
    B = 10e-9; % not used
    n = [0.005 0.005]*1e6;
    T = [700 220]; T_K = T*units.eV/kB; % use parallel temperature
    m = [1 1]*me;
    q = [-1 -1]*qe; 
    vd = [2700 16000]*1e3; % m/s
  case 'slow_i'
    
  case 'fast_e'
    B = 10e-9; % not used
    n = [0.005 0.004]*1e6;
    T = [400 600]; T_K = T*units.eV/kB; % use parallel temperature
    m = [1 1]*me;
    q = [-1 -1]*qe; 
    vd = [-000 30000]*1e3; % m/s
  case 'fast_i'
     
  case 'fast'
    
end

varargout{1} = n;
varargout{2} = T;
varargout{3} = m;
varargout{4} = q;
varargout{5} = vd;
